{% if navdata[nav] %}
<ul id="nav">
    {%- for name, link in navdata[nav] %}
        {% if page.url == link %}
            <li><a href="{{ link | url }}" aria-current="page"><b>{{ name }}</b></a></li>
        {% else %}
            <li><a href="{{ link | url }}">{{ name }}</a></li>
        {% endif %}
    {% endfor -%}
</ul>
{% endif %}